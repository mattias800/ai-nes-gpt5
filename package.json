{
  "name": "ai-nes-gpt5",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "cross-env PPU_TIMING_DEFAULT=vt vitest run --coverage",
    "test:fast": "cross-env PPU_TIMING_DEFAULT=vt vitest run --config vitest.fast.config.ts",
    "test:slow": "cross-env SMB3_BASELINE_CRC= SMB3_DEEP_BASELINE_CRC= PPU_TIMING_DEFAULT=vt vitest run tests/slow/**/*.test.ts",
    "test:blargg": "cross-env PPU_TIMING_DEFAULT=vt vitest run --config vitest.slow.config.ts",
    "test:blargg:quick": "cross-env BLARGG_QUICK=1 PPU_TIMING_DEFAULT=vt vitest run --config vitest.slow.config.ts",
    "verify": "npm run test:fast && npm run test:slow",
    "dev": "vite",
    "build:host": "vite build",
    "preview": "vite preview",
    "test:watch": "vitest",
    "test:long-run": "LONG_RUN=1 vitest run tests/harness/long_run_mario.test.ts",
    "test:long-run-deep": "LONG_RUN=1 vitest run tests/harness/long_run_mario_deep.test.ts",
    "test:long-run-trace": "TRACE=1 TRACE_MAX=50000 LONG_RUN=1 vitest run --reporter=verbose tests/slow/harness/long_run_mario_trace.test.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "fetch:roms": "node scripts/fetch-roms.mjs",
    "screenshot": "SCREENSHOT=1 vitest run tests/slow/harness/screenshot.test.ts",
    "gif": "tsx -r tsconfig-paths/register scripts/make-gif.ts",
    "baseline:smb": "node scripts/gen-smb-baseline.mjs",
    "baseline:smb3": "node scripts/gen-smb3-baseline.mjs",
    "baseline:smb3:state": "node scripts/gen-smb3-state-baseline.mjs",
    "baseline:smb3:input": "node scripts/gen-smb3-input-baseline.mjs",
    "baseline:smb3:input:extended": "node scripts/gen-smb3-input-extended-baseline.mjs",
    "accuracy:report": "node scripts/accuracy-report.mjs",
    "nestest:trace": "tsx -r tsconfig-paths/register scripts/run-nestest-trace.ts",
    "nestest:diff": "mkdir -p out && npm run nestest:trace -- --max=${NESTEST_MAX:-0} > out/nestest.trace && diff -u roms/nestest.log out/nestest.trace || true",
    "nestest:compare": "tsx -r tsconfig-paths/register scripts/compare-nestest.ts",
    "dump:wav": "tsx -r tsconfig-paths/register scripts/dump-wav.ts",
    "nestest:fetch": "node scripts/fetch-nestest-assets.mjs",
    "nestest:validate:canonical": "node scripts/validate-nestest-log.mjs roms/nestest.log",
    "battery:export": "tsx -r tsconfig-paths/register scripts/battery-export.ts",
    "battery:import": "tsx -r tsconfig-paths/register scripts/battery-import.ts",
    "trace:rom": "tsx -r tsconfig-paths/register scripts/trace-rom.ts",
    "compare:log": "tsx -r tsconfig-paths/register scripts/compare-against-log.ts",
    "trace:external": "node scripts/generate-trace-fceux.mjs",
    "compare:external": "npm run compare:log -- --log=out/external-fceux.log",
    "trace:mame": "node scripts/generate-trace-mame.mjs",
    "compare:mame": "npm run compare:log -- --log=out/external-mame.log",
    "trace:mame:regs": "node scripts/generate-trace-mame-regs.mjs",
    "compare:mame:regs": "npm run compare:log -- --log=out/external-mame-regs.log",
    "trace:mame:nestest-auto": "START=C000 OUT=out/nestest-mame-auto.log node scripts/generate-trace-mame.mjs",
    "compare:mame:auto": "npm run compare:log -- --log=out/nestest-mame-auto.log",
    "mame:dump": "node scripts/generate-mame-dump.mjs",
    "mario:sync": "node scripts/chain-mario-compare.mjs",
    "nestest:sync": "node scripts/chain-nestest-compare.mjs",
    "scan:roms": "tsx -r tsconfig-paths/register scripts/scan-roms.ts --dir=${ROM_DIR:-roms}"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.9.0",
    "@eslint/js": "^9.9.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^5.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "pngjs": "^7.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1",
    "cross-env": "^7.0.3",
    "tsx": "^4.16.2",
    "tsconfig-paths": "^4.2.0"
  }
}
